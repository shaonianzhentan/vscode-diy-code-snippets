{
	"name": "diy-code-snippets",
	"displayName": "自定义代码片段",
	"description": "自定义代码片段",
	"keywords": [
		"自定义代码片段",
		"自定义代码",
		"代码片段"
	],
	"version": "0.0.5",
	"publisher": "shaonianzhentan",
	"repository": "https://github.com/shaonianzhentan/vscode-diy-code-snippets",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Snippets"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:vue",
		"onCommand:extension.exec"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "自定义代码片段",
			"properties": {
				"command": {
					"type": "string",
					"default": "",
					"description": "cmd命令（会在后面传入当前编辑文件的绝对路径）"
				}
			}
		},
		"keybindings": [
			{
				"command": "extension.exec",
				"key": "ctrl+alt+shift+z",
				"mac": "ctrl+alt+shift+z",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "extension.exec",
				"title": "调用自定义程序"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"bugs": {
		"url": "https://github.com/shaonianzhentan/vscode-diy-code-snippets/issues"
	},
	"homepage": "https://github.com/shaonianzhentan/vscode-diy-code-snippets/blob/master/README.md"
}