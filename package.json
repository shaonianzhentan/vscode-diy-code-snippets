{
	"name": "diy-code-snippets",
	"displayName": "自定义代码片段",
	"description": "自定义代码片段",
	"keywords": [
		"自定义代码片段",
		"自定义代码",
		"代码片段"
	],
	"publisher": "shaonianzhentan",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Snippets"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:vue",
		"onCommand:extension.exec"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "自定义代码片段",
			"properties": {
				"command": {
					"type": "string",
					"default": "",
					"description": "cmd命令（会在后面传入当前编辑文件的绝对路径）"
				}
			}
		},
		"keybindings": [
			{
				"command": "extension.exec",
				"key": "ctrl+alt+z",
				"mac": "ctrl+alt+z",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "extension.exec",
				"title": "调用自定义程序"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"bugs": {
		"url": "https://github.com/shaonianzhentan/vscode-diy-code-snippets/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shaonianzhentan/vscode-diy-code-snippets"
	},
	"homepage": "https://github.com/shaonianzhentan/vscode-diy-code-snippets/blob/master/README.md"
}